{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 350, 
    "height": 350, 
    "background": null, 
    "title": "Dental Therapists Over the Years",
    "data": {
      "url": "https://raw.githubusercontent.com/afia-farzana167/Code/refs/heads/main/Data/dentaTherapists.csv"
    }
    "mark": {
      "type": "trail",
      "point": {"filled": true, "size": 100}
    },
    "encoding": {
      "x": {
        "field": "Year",
        "type": "quantitative",
        "title": "Year"
      },
      "y": {
        "field": "Number",
        "type": "quantitative",
        "title": "Number of Dental Therapists"
      },
      "color": {
        "field": "Continent",
        "type": "nominal",
        "scale": {"scheme": "category10"}
      },
      "tooltip": [
        {"field": "Continent", "title": "Continent"},
        {"field": "Year", "title": "Year"},
        {"field": "Number", "title": "Number"}
      ]
    }
  }