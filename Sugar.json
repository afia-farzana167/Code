{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 750, 
    "height": 300, 
    "background": null, 
    "title": {
      "text": "Daily Availability of Sugar",
      "fontSize": 24,  
      "fontWeight": "normal",
      "font": "Times New Roman", 
      "fontStyle": "italic",
      "anchor": "middle"  
    },
    "data": {
      "url": "https://raw.githubusercontent.com/afia-farzana167/Code/refs/heads/main/Data/SugarPerCapita.csv"
    },
    "layer": [
      {
        "mark": {
          "type": "rule",
          "color": "#CFB1A9", 
          "size": 15
        },
        "encoding": {
          "x": {
            "field": "ParentLocation", 
            "type": "nominal"},
          "y": {
            "field": "FactValueNumeric", 
            "type": "quantitative", 
            "scale": {"reverse": false}
          } 
        }
      },
      {
        "mark": {
          "type": "image",
          "width": 80, 
          "height": 80, 
          "tooltip": true 
        },
        "encoding": {
          "url": {
            "field": "Lollipop", 
            "type": "nominal"
          },
          "x": {
            "field": "ParentLocation", 
            "type": "nominal", 
            "axis": {
              "ticks": false,
              "labels": false,
              "title": null,
              "titlePadding": 10
            }
          },
          "y": {
            "field": "FactValueNumeric", 
            "type": "quantitative", 
            "scale": {
              "domain": [0, 5000] 
            }, 
            "axis": {
              "title": "Availability of Sugar Per Capita (g/day)",
              "tickCount": 5, 
              "titleFont": "Times New Roman", 
              "titleFontSize": 16,
              "titleFontWeight": "normal", 
              "titlePadding": 10
            }
          },
          "tooltip": [
            {"field": "ParentLocation", "type": "nominal", "title": "Continent"},
            {"field": "FactValueNumeric", "type": "quantitative", "title": "Amount of Sugar (g/day)"}
          ]
        }
      }
    ]
  }
  